{
  "name": "formular.dev",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "vendors/*"
  ],
  "scripts": {
    "build": "nx run-many --target=build --all",
    "build:lib": "nx build formular-dev-lib",
    "build:components": "nx build formular-components",
    "dev": "nx run-many --target=dev --all --parallel",
    "dev:lib": "nx dev formular-dev-lib",
    "dev:components": "nx dev formular-components",
    "test": "nx run-many --target=test --all",
    "test:lib": "nx test formular-dev-lib",
    "test:components": "nx test formular-components",
    "test:coverage": "nx run-many --target=test:coverage --all",
    "graph": "nx graph",
    "affected:build": "nx affected --target=build",
    "affected:test": "nx affected --target=test",
    "affected:graph": "nx affected:graph",
    "reset": "nx reset"
  },
  "devDependencies": {
    "@nx/jest": "^21.2.0",
    "@nx/react": "^21.2.0",
    "@nx/vite": "^21.2.0",
    "concurrently": "^8.2.2",
    "nx": "21.2.0",
    "pnpm": "^8.15.9",
    "typescript": "^5.8.3"
  }
}