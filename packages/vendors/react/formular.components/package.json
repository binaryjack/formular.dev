{
    "name": "formular.components",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "env-cmd -f ./.env.development --use-shell vite",
        "start:dev": "env-cmd -f ./.env.development --use-shell vite",
        "start:integration": "env-cmd -f ./.env.integration --use-shell vite",
        "start:production": "env-cmd -f ./.env.production --use-shell vite",
        "build": "tsc",
        "build:dev": "env-cmd -f ./.env.development --use-shell vite build",
        "build:integration": "env-cmd -f ./.env.integration --use-shell vite build",
        "build:production": "env-cmd -f ./.env.production --use-shell vite build",
        "build-css": "tailwindcss build -i ./src/index.css -o ./public/index.css",
        "sta": "tsc-watch",
        "watch": "tsc -w",
        "lint": "eslint",
        "serve": "vite preview",
        "serve:dev": "env-cmd -f ./.env.development --use-shell vite preview",
        "serve:integration": "env-cmd -f ./.env.integration --use-shell vite preview",
        "serve:production": "env-cmd -f ./.env.production --use-shell vite preview",
        "tests:cypress": "cypress open"
    },
    "dependencies": {
        "dompurify": "^3.2.6",
        "formular.dev.lib": "workspace:^1.0.56",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-icons": "^5.5.0",
        "shared-assets": "workspace:^1.0.0"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.26.9",
        "@babel/preset-typescript": "^7.26.0",
        "@emotion/babel-plugin": "^11.13.5",
        "@emotion/css": "^11.13.5",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@jest/globals": "^29.7.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/cypress": "^1.1.6",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.15.31",
        "@types/prismjs": "^1.26.5",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@vitejs/plugin-react-swc": "^3.7.1",
        "autoprefixer": "^10.4.20",
        "cypress": "^14.3.1",
        "eslint": "^9.13.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-playwright-preset": "^4.0.0",
        "postcss": "^8.4.49",
        "postcss-import": "^16.1.0",
        "postcss-nesting": "^13.0.1",
        "prettier": "^3.3.3",
        "prismjs": "^1.30.0",
        "prompts": "^2.4.2",
        "slugify": "^1.6.6",
        "tailwindcss": "^3.4.16",
        "ts-jest": "^29.2.6",
        "ts-node": "^10.9.2",
        "typescript": "^5.6.3",
        "vite": "^5.4.10",
        "vite-plugin-checker": "^0.8.0",
        "vite-plugin-dts": "^4.3.0",
        "vite-plugin-eslint": "^1.8.1",
        "vite-plugin-static-copy": "^2.0.0",
        "vite-plugin-svg": "^0.7.0"
    }
}
